/* ------------------------------------------------------------ *\
	Header
\* ------------------------------------------------------------ */
.header { position: relative; text-align: center; position: fixed; left: 0; width: 100%; top: 0; padding: 0 7.5%; z-index: 2; transition: background .3s; }
.header .header__inner { padding: 112px 0 87px; transition: padding .3s; }
.header .logo { position: absolute; top: 60px; left: 50%; -ms-transform: translateX(-50%); transform: translateX(-50%); }
.header.fixed { background: #13191b; }
.header.fixed .logo {width: 185px; height: 49px; top: 39px;}
.header.fixed .header__inner { padding: 56px 0 34px; }
.header.fixed .nav--dark { color: #F1F1F1; }
/*  header white  */
.header--white { background: #ffffff; }
.header--alt { position: relative; }
.header--alt .header__inner { padding: 112px 0 89px; }
.header--alt.fixed { position: fixed; }
.header--alt.fixed ~ .main { padding-top: 126px; }
/* Small Desktop */
@media (max-width: 1279px) {
	.header { padding: 0 50px; }
	.header .logo { top: 30px; }
	.header .header__inner { padding: 80px 0 70px; }
}
/* Tablet */
@media (max-width: 1023px) {
	.header { padding: 30px; }
	.header .header__inner { padding: 42px 0 25px; }
	.header .logo { top: 30px; }

	.header.fixed .header__inner { padding: 24px 0 12px; }
}
/* Mobile */
@media (max-width: 767px) {
	.header { padding: 38px 20px; }
	.header .nav { margin-top: 0; }
	.header .header__inner { padding: 0; display: -ms-flexbox; display: flex; -ms-flex-pack: justify; justify-content: space-between; -ms-flex-align: center; align-items: center; }

	.header.fixed .logo { width: 112px; height: 30px; top: 30px; background-image: url(images/logo-mobile@2x.png); }
	.header.fixed .header__inner { padding: 0; }

	.header--alt.fixed ~ .main { padding-top: 90px; }
}

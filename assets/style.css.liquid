.footer .footer__actions { display: -ms-flexbox; display: flex; -ms-flex: 1; flex: 1; }
.footer .footer__nav { font-weight: 700; letter-spacing: 0.3em; text-transform: uppercase; padding: 43px 50px 41px;  display: -ms-flexbox;  display: flex; -ms-flex-align: center; align-items: center; -ms-flex-pack: center; justify-content: center; }
.footer .footer__nav li + li { margin-top: 32px; }
.footer .footer__nav a { text-decoration: none; transition: color .15s; }

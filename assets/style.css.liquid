.footer .footer__col { display: -ms-flexbox; display: flex; -webkit-box-orient: vertical; -webkit-box-direction: normal; -ms-flex-flow: column; flex-flow: column; -ms-flex-wrap: wrap; flex-wrap: wrap; }
.footer .footer__content { height: 100%; }

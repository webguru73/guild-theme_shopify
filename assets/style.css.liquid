.shell { max-width: 978px; padding-left: 15px; padding-right: 15px; margin: auto; }
.shell--secondary { max-width: 1266px; }
.shell--large { max-width: 1252px; }
.shell--larger  { max-width: 1278px; }
.shell-fluid { max-width: none; }

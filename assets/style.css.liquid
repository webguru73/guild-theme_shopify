.footer .form { display: inline-block; -ms-flex: 0 0 61%; flex: 0 0 61%; margin-left: 20px; margin-top: -2px;}
.footer .footer__aside { text-align: left; padding: 101px 11.5% 151px; }
.footer .footer__aside + .footer__actions { border-top: 2px solid #45758B; }
